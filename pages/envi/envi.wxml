<!--pages/envi/envi.wxml-->

<view class="container">


  <scroll-view scroll-y style="height: 800px;" bindscrolltoupper="upper" bindscrolltolower="lower" bindscroll="scroll" scroll-into-view="{{toView}}" scroll-top="{{scrollTop}}">
    <view wx:for="{{enviinfo}}">
      <text>Temperature:{{item.Tem}}\n
    Humidity:{{item.Hum}}\n
    Pressure:{{item.Pre}}\n
    Time:{{item.Timestamp}}\n
    ------------------------------\n
    </text>
    </view>

  </scroll-view>


</view>